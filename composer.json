{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^2.3",
        "drupal/backup_migrate": "^4.1",
        "drupal/better_exposed_filters": "^5.0@beta",
        "drupal/block_class": "^1.2",
        "drupal/block_visibility_groups": "^1.3",
        "drupal/captcha": "^1.1",
        "drupal/chosen": "^3.0",
        "drupal/clamav": "^2.0",
        "drupal/cloudflare": "^1.0",
        "drupal/config_split": "^1.5",
        "drupal/config_update": "^1.7",
        "drupal/contact_block": "^1.5",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/devel": "^4.0",
        "drupal/dropzonejs": "^2.3",
        "drupal/drupal_moodle_integration": "^3.0",
        "drupal/entity_clone": "^1.0",
        "drupal/entityqueue": "^1.1",
        "drupal/environment_indicator": "^4.0",
        "drupal/features": "^3.11",
        "drupal/feeds": "^3.0",
        "drupal/field_group": "^3.1",
        "drupal/ga_login": "^1.0",
        "drupal/google_analytics": "^3.1",
        "drupal/google_map_field": "^1.14",
        "drupal/honeypot": "^2.0",
        "drupal/imageapi_optimize": "^4.0",
        "drupal/imagemagick": "^3.1",
        "drupal/imce": "^2.3",
        "drupal/inline_entity_form": "^1.0",
        "drupal/lightning_api": "^4.6",
        "drupal/lightning_layout": "^2.7",
        "drupal/lightning_workflow": "^3.15",
        "drupal/media_bulk_upload": "^1.0",
        "drupal/media_entity_facebook": "^2.0",
        "drupal/media_entity_instagram": "^3.0",
        "drupal/media_entity_slideshow": "^2.0",
        "drupal/media_entity_twitter": "^2.4",
        "drupal/moderated_content_bulk_publish": "^2.0",
        "drupal/olivero": "^1.0",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/purge": "^3.0",
        "drupal/purge_queuer_url": "^1.0",
        "drupal/real_aes": "^2.3",
        "drupal/recaptcha": "^3.0",
        "drupal/seckit": "^2.0",
        "drupal/shield": "^1.4",
        "drupal/simplenews": "^2.0",
        "drupal/sitemap": "^2.0",
        "drupal/smtp": "^1.0",
        "drupal/social_auth_google": "^2.1",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/tfa": "^1.0",
        "drupal/tvi": "^1.0",
        "drupal/views_infinite_scroll": "^1.7",
        "drupal/webform": "^6.0",
        "drupal/xmlsitemap": "^1.0",
        "drush/drush": "^10.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
